    "build": "tsc",
    "start": "tsc && node dist/index.js",
    "lint": "tslint --project .",
    "test": "nyc --reporter=text mocha -r ts-node/register ./src/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdityaRanjanSingh/covid-19-dialog-flow-codehook.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AdityaRanjanSingh/covid-19-dialog-flow-codehook/issues"
  },
  "homepage": "https://github.com/AdityaRanjanSingh/covid-19-dialog-flow-codehook#readme",
  "dependencies": {
    "@types/inversify": "^2.0.33",
    "@types/node": "^13.11.1",
    "axios": "^0.19.2",
    "inversify": "^5.0.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.8.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "mocha": "^7.1.1",
    "nyc": "^15.0.1",
    "pre-commit": "^1.2.2",
    "sinon": "^9.0.2",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js",
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.spec.js",
      "**/*.spec.ts"
    ]
